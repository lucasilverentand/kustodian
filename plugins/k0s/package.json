{
  "name": "kustodian-k0s",
  "version": "1.2.1",
  "description": "k0s cluster provider plugin for Kustodian",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun run tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": ["kustodian", "plugin", "k0s", "kubernetes", "cluster"],
  "author": "Luca Silverentand <luca@onezero.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasilverentand/kustodian.git",
    "directory": "plugins/k0s"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "yaml": "^2.8.2"
  },
  "peerDependencies": {
    "kustodian": "^1.0.0"
  },
  "devDependencies": {}
}
