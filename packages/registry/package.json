{
  "name": "@kustodian/registry",
  "version": "1.1.1",
  "description": "Container registry client for fetching image tags from Docker Hub and GHCR",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun run tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "kustodian",
    "registry",
    "docker",
    "ghcr",
    "container"
  ],
  "author": "Luca Silverentand <luca@onezero.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasilverentand/kustodian.git",
    "directory": "packages/registry"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@kustodian/core": "workspace:*",
    "semver": "^7.6.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/semver": "^7.5.0"
  }
}
