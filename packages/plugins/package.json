{
  "name": "@kustodian/plugins",
  "version": "2.0.0",
  "description": "Plugin system infrastructure for Kustodian",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun run tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "kustodian",
    "plugins"
  ],
  "author": "Luca Silverentand <luca@onezero.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasilverentand/kustodian.git",
    "directory": "packages/plugins"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@kustodian/core": "workspace:*",
    "@kustodian/nodes": "workspace:*",
    "@kustodian/schema": "workspace:*",
    "zod": "^3.25.76"
  },
  "devDependencies": {}
}
