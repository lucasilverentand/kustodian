{
  "name": "@kustodian/schema",
  "version": "1.0.0",
  "description": "JSON Schema definitions for Kustodian YAML validation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun run tsc --build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun run tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": ["kustodian", "schema", "validation", "zod"],
  "author": "Luca Silverentand <luca@onezero.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasilverentand/kustodian.git",
    "directory": "packages/schema"
  },
  "dependencies": {
    "zod": "^3.25.30"
  },
  "devDependencies": {}
}
