{
  "name": "@kustodian/core",
  "version": "1.1.0",
  "description": "Core utilities, error handling, and Result type for Kustodian",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun run tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "kustodian",
    "core",
    "utilities"
  ],
  "author": "Luca Silverentand <luca@onezero.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasilverentand/kustodian.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {}
}
