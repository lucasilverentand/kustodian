{
  "name": "@kustodian/bootstrap",
  "version": "1.0.0",
  "description": "Cluster bootstrap orchestration for Kustodian",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run tsc --build",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun run tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "keywords": [
    "kustodian",
    "bootstrap",
    "kubernetes"
  ],
  "author": "Luca Silverentand <luca@onezero.company>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasilverentand/kustodian.git",
    "directory": "packages/bootstrap"
  },
  "dependencies": {
    "@kustodian/core": "workspace:*",
    "@kustodian/nodes": "workspace:*",
    "@kustodian/schema": "workspace:*"
  },
  "devDependencies": {}
}
